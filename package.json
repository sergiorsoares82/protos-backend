{
  "name": "protos-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "npm run services:up && tsx watch src/index.ts",
    "build": "prisma generate && tsup src/index.ts --dts --format esm,cjs",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.js",
    "services:up": "docker compose -f src/infra/compose.yaml up -d",
    "services:stop": "docker compose -f src/infra/compose.yaml stop",
    "services:down": "docker compose -f src/infra/compose.yaml down",
    "lint:prettier:check": "prettier --check .",
    "lint:prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiorsoares82/protos-backend.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergiorsoares82/protos-backend/issues"
  },
  "homepage": "https://github.com/sergiorsoares82/protos-backend#readme",
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.30",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prisma": "^6.9.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}
